class Reply(SimpleHTTPServer.SimpleHTTPRequestHandler):

  def do_GET(self):
    split = self.path.split('/')
    twitter_handle = split[len(split) - 1]
    data = api.GetUserTimeline(twitter_handle)
    return str(api.GetUserTimeline(twitter_handle))

def main():
